									<spacer style="height:3px;" />
								</rows>
							</grid>

							<grid flex="1">
								<columns>
									<column flex="1" />
									<column />
									<column />
									<column />
									<column />
								</columns>

								<rows>
									<row>
										<hbox	id="simulateSubjectParsingHBox"
