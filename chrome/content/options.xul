									<spacer style="height:10px;" />

									<row>
										<listbox	id="subjectsListBox"
													height="100"
													onselect="onSelectSubject();" />
										<grid flex="1">
											<rows>
												<row flex="1" />
												<row />
												<row flex="1" />
												<row />
												<row flex="1" />
												<row />
												<row flex="1" />
